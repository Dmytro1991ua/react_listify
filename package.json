{
  "name": "react_listify",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "engines": {
    "npm": "6.14.13",
    "node": "14.17.0"
  },
  "scripts": {
    "build": "npx tsc -p .",
    "start": "npm install && npm run build && node server/dist/index.js",
    "server": "ts-node-dev --files --respawn --pretty --transpile-only server/index.ts",
    "client": "npm run dev --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "render-postbuild": "cd client && npm install --only=dev && npm install && npm run build"
  },
